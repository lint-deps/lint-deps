{
  "name": "lint-deps",
  "description": "CLI tool that tells you when dependencies are missing from package.json and offers you a choice to install them. Also tells you when dependencies are listed in package.json but are not being used anywhere in your project. Node.js command line tool and API.",
  "version": "5.0.0-beta",
  "homepage": "https://github.com/lint-deps/lint-deps",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "hemanth.hm (http://h3manth.com)",
    "Jake Verbaten (http://github.com/Raynos)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "Mark Dalgleish (markdalgleish.com)"
  ],
  "repository": "lint-deps/lint-deps",
  "bugs": {
    "url": "https://github.com/lint-deps/lint-deps/issues"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "deps": "bin/cli.js",
    "lint-deps": "bin/cli.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "mocha"
  },
  "preferGlobal": true,
  "dependencies": {
    "@babel/parser": "^7.18.5",
    "@folder/xdg": "^3.1.2",
    "ansi-colors": "^4.1.1",
    "cross-spawn": "^7.0.3",
    "dirent": "^2.0.1",
    "enquirer": "^2.3.6",
    "find-pkg": "^2.0.0",
    "get-value": "^3.0.1",
    "inflection": "^1.13.1",
    "minimist": "^1.2.5",
    "path-starts-with": "^2.0.0",
    "picomatch": "^2.2.3",
    "strip-comments": "^2.0.1",
    "typescript": "^4.6.3",
    "validate-npm-package-name": "^3.0.0",
    "write": "^2.0.0"
  },
  "keywords": [
    "dep",
    "dependencies",
    "dependency",
    "deps",
    "detect",
    "detective",
    "devDependencies",
    "extra",
    "find",
    "installed",
    "lint",
    "missing",
    "new",
    "require",
    "required",
    "requires",
    "unused"
  ],
  "eslintConfig": {
    "rules": {
      "no-console": 0
    }
  },
  "verb": {
    "toc": "collapsible",
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
